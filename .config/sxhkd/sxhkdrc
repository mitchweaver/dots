# programs
alt Return
    stwrapper
#   kitty
alt + w
    brws
alt + r
    stwrapper -e ranger
    # kitty --single-instance ranger

super + l
    xautolock -locknow -locker 'i3lock-fancy-rapid 15 pixel'

Print
    scrap -n
control + Print
    scrap

alt + e
    pcmanfm

# dmenus
alt + p
    menu --wide run -p 'Run:'
alt + o
    dedit
# alt + y
    # dmoji

# brightness
XF86MonBrightnessUp
    bright i
XF86MonBrightnessDown
    bright d

# volume
alt + apostrophe
    vol i
alt + semicolon
    vol d
XF86VolumeUp
    vol i
XF86VolumeDown
    vol d

# media / mpv
alt + slash
    mmt -t
alt + period
    mmt -n
alt + comma
    mmt -p
alt + bracketright
    mmt -f
alt + bracketleft
    mmt -b
